
# 25478
# :25478
# http://127.0.0.1:25478
# https://127.0.0.1:25478
# default :25478

listen = ":25478"

# tls crt and key file
# default empty

crt = "srv.crt"
key = "srv.key"

# simple json store
# default path ./data
# default days 0, no housekeeping

[jsonstore]
path = "./data"
days = 30

# pushover config

[pushover]
  app = "API Token"
  user = "Your User Key"
  